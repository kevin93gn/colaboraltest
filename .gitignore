# Ignore bundler config
/.bundle
/gems
/.gemtags
/.tags
/update_ctags.rb
/config/locales/*_*.yml

/frontend

/.sass-cache
/SocialReferral.sublime-workspace

/public/*
/vendor/bundle/*

# Ignore vagrant config
/.vagrant

# Ignore local history
/.pry_history

# Ignore the shared apt-cache
/.apt-cache

/.vscode

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log*
/config/locales/log/
/log/localeapp.yml
/tmp
/chromedriver.log
/test/tmp/*.png
/test/tmp/*.html


# Ignore public assets
/public/*
/app/views/root/*.erb

# Ignore configuration changes
/config/mongoid.yml
/config/stormtroopers.yml
/config/app_config.yml
/config/my_supply.yml
/config/cb.yml
/config/credentials.yml


# Ignore Mac OS X garbage files
*.DS_Store
.idea

# Ignore ctags tag file
tags

vendor/cache/*darwin*.gem

config/database.yml
git.sh

ionic-app/

*.old
node_modules

/coverage/
!/coverage/.gitkeep
docker-compose.override.yml
